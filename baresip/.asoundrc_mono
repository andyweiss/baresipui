
# ALSA configuration for RAVENNA â€“ single shared dmix/dsnoop, plus mono route devices per channel
# Numbering starts at 1 (in_ch1..in_ch64, out_ch1..out_ch64)
#
# This file exposes:
#  - pcm.ravenna_shared_out : shared playback to hw:CARD=RAVENNA (64ch)
#  - pcm.ravenna_shared_in  : shared capture  from hw:CARD=RAVENNA (64ch)
#
# Notes:
#  - Channel indexing in ALSA ttable remains zero-based internally, but names start at 1.
#  - Keep period/buffer timings aligned to your Ravenna engine; adjust if needed.
#  - Format set to S16_LE to match provided config; change to S24_3LE or S32_LE if your workflow requires.

pcm.ravenna_shared_out {
    type dmix
    ipc_key 0x999999
    slave {
        pcm "hw:CARD=RAVENNA"
        channels 64
        rate 48000
        format S16_LE
        period_time 2500
        buffer_time 10000
        periods 4
    }
}

pcm.ravenna_shared_in {
    type dsnoop
    ipc_key 0x888888
    slave {
        pcm "hw:CARD=RAVENNA"
        channels 64
        rate 48000
        format S16_LE
        period_time 2500
        buffer_time 10000
        periods 4
    }
}


# Mono capture: RAVENNA channel 1
pcm.in_ch1 {
    type route
    slave {
        pcm "ravenna_shared_in"
        channels 64
    }
    ttable {
        0.0  1.0
    }
}

# Mono playback: RAVENNA channel 1
pcm.out_ch1 {
    type route
    slave {
        pcm "ravenna_shared_out"
        channels 64
    }
    ttable {
        0.0  1.0
    }
}


