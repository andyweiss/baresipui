# Single shared dmix/dsnoop for ALL channels - no conflicts
pcm.ravenna_shared_out {
    type dmix
    ipc_key 0x999999
    slave {
        pcm "hw:CARD=RAVENNA"
        channels 64
        rate 48000
        format S16_LE
        period_time 2500
        buffer_time 10000
        periods 4
    }
}

pcm.ravenna_shared_in {
    type dsnoop
    ipc_key 0x888888
    slave {
        pcm "hw:CARD=RAVENNA"
        channels 64
        rate 48000
        format S16_LE
        period_time 2500
        buffer_time 10000
        periods 4
    }
}

# Route specific channel pairs from the shared devices
pcm.in1_2 {
    type route
    slave {
        pcm "ravenna_shared_in"
        channels 64
    }
    ttable {
        0.0  1.0
        1.1  1.0
    }
}

pcm.out1_2 {
    type route
    slave {
        pcm "ravenna_shared_out" 
        channels 64
    }
    ttable {
        0.0  1.0
        1.1  1.0
    }
}

